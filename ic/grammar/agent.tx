Agent:
    '#' name=ID 
    ('crencas:' beliefs=NameList)?
    ('objetivos:' desires=NameList)?
    ('planos:' plans*=Plans+[','])? ';'
;

NameList:
    list=ID+[','] ';'
;

Plans:
    name=ID '(' 
    'quando:' data=ID 
    (';' 'contem:' conditions=ConditionList)? ')' 
    '{' actions=ActionList? '}'
;

ConditionList:
    condition=ID+[',']
;

ActionList:
    action=ID+[',']
;