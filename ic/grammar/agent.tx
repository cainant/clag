NAGAgent:
    '#' name=ID 
    ( ('crencas:' beliefs*=ID[','])
    ('objetivos:' desires*=ID[','])
    ('planos:' plans*=Plans+[',']) )#
;

Plans:
    name=ID '(' 
    'quando:' data=Action 
    ('contem:' conditions*=Condition[','])? ')' 
    '{' actions*=Action[','] '}'
;

Condition:
    ctxType=ContextType name=ID
;

Action:
    actType=ActionType name=ID
;

ActionType:
    'acreditar' | 'desacreditar' | 'cumprir'
;

ContextType:
    'acredita'
;
