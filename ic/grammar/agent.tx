NAGAgent:
    '#' name=ID 
    ('crencas:' beliefs*=ID[','] ';')?
    ('objetivos:' desires*=ID[','] ';')?
    ('planos:' plans*=Plans+[','])? ';'
;

Plans:
    name=ID '(' 
    'quando:' data=ID 
    (';' 'contem:' conditions=ConditionList)? ')' 
    '{' actions=ActionList? '}'
;

ConditionList:
    condition=ID+[',']
;

ActionList:
    action=ID+[',']
;